{
    "openapi": "3.0.0",
    "info": {
        "title": "Laravel OpenApi Documentação - Swagger",
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "http://localhost:8000/api/auth/"
        }
    ],
    "paths": {
        "/login": {
            "post": {
                "tags": [
                    "Usuario"
                ],
                "summary": "Login para pegar token JWT",
                "operationId": "67d6d0fdd54fba132e1b6f55ca254183",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email",
                                    "password"
                                ],
                                "properties": {
                                    "email": {
                                        "type": "string"
                                    },
                                    "password": {
                                        "type": "string"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "OK"
                    },
                    "401": {
                        "description": "Unauthorized"
                    },
                    "404": {
                        "description": "Not Found"
                    }
                }
            }
        },
        "/register": {
            "post": {
                "tags": [
                    "Usuario"
                ],
                "summary": "Cadastro de Usuário",
                "operationId": "5d4fa5d5607ed675ba93907e1fc94534",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "name",
                                    "email",
                                    "password",
                                    "password_confirmation"
                                ],
                                "properties": {
                                    "name": {
                                        "type": "string"
                                    },
                                    "email": {
                                        "type": "string"
                                    },
                                    "password": {
                                        "type": "string"
                                    },
                                    "password_confirmation": {
                                        "type": "string"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "OK"
                    },
                    "404": {
                        "description": "Not Found"
                    }
                }
            }
        },
        "/paciente/store": {
            "post": {
                "tags": [
                    "Paciente"
                ],
                "summary": "Paciente store",
                "operationId": "e220b96ab5da13dda5f90fa84f29966a",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "pac_nome",
                                    "pac_dataNascimento"
                                ],
                                "properties": {
                                    "pac_nome": {
                                        "type": "string"
                                    },
                                    "pac_dataNascimento": {
                                        "type": "string"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "OK"
                    },
                    "401": {
                        "description": "Unauthorized"
                    },
                    "404": {
                        "description": "Not Found"
                    }
                },
                "security": [
                    {
                        "bearer_token": []
                    }
                ]
            }
        },
        "/paciente/show": {
            "get": {
                "tags": [
                    "Paciente"
                ],
                "summary": "Paciente show",
                "operationId": "5073264e17dc29cea047696ff50d2b21",
                "responses": {
                    "200": {
                        "description": "OK"
                    },
                    "404": {
                        "description": "Not Found"
                    }
                },
                "security": [
                    {
                        "bearer_token": []
                    }
                ]
            }
        }
    },
    "components": {
        "securitySchemes": {
            "bearer_token": {
                "type": "apiKey",
                "description": "Para realizar login adione Bearer + o token. Exemplo: Bearer hjfdsfkhjhjerryue",
                "name": "Authorization",
                "in": "header"
            }
        }
    }
}